{
    "大家乐": {    // 该小票配置名称，可以随便取
        "date": "\\d{2}/\\d{2}/\\d{4}",    // 日期正则表达式
        "hms": "\\d+:\\d+",    // 时间正则表达式
        "order_keyword": "单号",    // 单号关键字
        "order": "\\d+",    // 单号格式
        "shop": "深圳星过盛世店",    // 店铺名称关键字
        "money_keyword": "现金",    // 实付金额关键字
        "money_offset": 1,    // 金额在数组中距离money_keyword偏移位置(不要根据原图片判断，一定要根据识别结果的图片判断)，1:前进一位，0:当前位置，-1:上一个位置
        "money_num": "\\d+\\.?\\d*"    // 金额正则表达式
    }
}